#include <stdio.h>
#include <limits.h>

#define MAX 20

int main() {
    int n;
    int graph[MAX][MAX];
    int selected[MAX];
    int edges = 0;
    int min, x = 0, y = 0;
    int totalCost = 0;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix (0 if no edge):\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &graph[i][j]);
        }
        selected[i] = 0;
    }

    /* Start from vertex 0 */
    selected[0] = 1;

    printf("\nEdges in Minimum Spanning Tree:\n");
