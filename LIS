#include <stdio.h>

int max(int a, int b) {
    return (a > b) ? a : b;
}

int LIS(int arr[], int n) {
    int dp[n];

    for (int i = 0; i < n; i++)
        dp[i] = 1;

    for (int i = 1; i < n; i++)
        for (int j = 0; j < i; j++)
            if (arr[i] > arr[j])
                dp[i] = max(dp[i], dp[j] + 1);

    int ans = 0;
    for (int i = 0; i < n; i++)
        ans = max(ans, dp[i]);

    return ans;
}

int main() {
    int arr[] = {10, 22, 9, 33, 21, 50, 41, 60};
    printf("LIS length = %d", LIS(arr, 8));
    return 0;
}
